
 <h2><span>注册新用户</span></h2>
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name),:html => {:class => "login"}) do |f| %>
        <p>
            <label for="user_username">用户名</label>
            <%= f.text_field :username , :class => "input-medium validate[required,minSize[6],maxSize[30],custom[onlyLetterNumber]]" %>
        </p>
        <p>
            <label for="user_email">电子邮件</label>
            <%= f.email_field :email , :class => "input-medium validate[required,maxSize[50],custom[email]]" %>
        </p>
        <p>
            <label for="user_password">密码</label>
            <%= f.password_field :password, :class => "input-medium validate[required,minSize[6],maxSize[30]]" %>
        </p>
        <p>
            <label for="user_password_confirmation">密码确认</label>
            <%= f.password_field :password_confirmation, :class => "input-medium validate[required,minSize[6],maxSize[30],equals[user_password]]" %>
        </p>
        <p>
            <input  type="submit" value="完成" /> 
            <input  type="submit" value="取消" />
        </p>
        <p>
          <%= render "devise/shared/links" %>
        </p>
    <% end %>
